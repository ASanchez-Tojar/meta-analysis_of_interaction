@ARTICLE{Zuppinger-Dingley2016918,
author={Zuppinger-Dingley, D. and Flynn, D.F.B. and De Deyn, G.B. and Petermann, J.S. and Schmid, B.},
title={Plant selection and soil legacy enhance long-term biodiversity effects},
