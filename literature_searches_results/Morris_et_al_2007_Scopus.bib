@ARTICLE{Lau201280,
author={Lau, J.A. and Bowling, E.J. and Gentry, L.E. and Glasser, P.A. and Monarch, E.A. and Olesen, W.M. and Waxmonsky, J. and Young, R.T.},
title={Direct and interactive effects of light and nutrients on the legume-rhizobia mutualism},
