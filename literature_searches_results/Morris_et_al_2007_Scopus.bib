@ARTICLE{Campanella200950,
author={Campanella, D.M. and McEvoy, P.B. and Mundt, C.C.},
title={Interaction effects of two biological control organisms on resistant and susceptible weed biotypes of Chondrilla juncea in western North America},
journal={Biological Control},
year={2009},
volume={50},
