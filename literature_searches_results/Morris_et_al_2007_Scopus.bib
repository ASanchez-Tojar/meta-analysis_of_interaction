@ARTICLE{Andersson20161175,
author={Andersson, P. and Ehrlén, J. and Hambäck, P.A.},
title={Plant patch structure influences plant fitness via antagonistic and mutualistic interactions but in different directions},
journal={Oecologia},
year={2016},
