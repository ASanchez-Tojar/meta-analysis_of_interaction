@ARTICLE{Stein2009191,
author={Stein, C. and Rißmann, C. and Hempel, S. and Renker, C. and Buscot, F. and Prati, D. and Auge, H.},
title={Interactive effects of mycorrhizae and a root hemiparasite on plant community productivity and diversity},
journal={Oecologia},
